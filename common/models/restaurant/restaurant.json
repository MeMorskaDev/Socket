{
    "name": "Restaurant",
    "prural": "Restaurants",
    "base": "BaseEntity",
    "properties": {
        "id": {
            "type": "string",
            "id": true
        },
        "restaurantName": {
            "type": "string",
            "required": true
        },
        "address": {
            "type": "Address",
            "required": true
        },
        "phoneNumber": {
            "type": "string"
        },
        "emailId": {
            "type": "string"
        },
        "website": {
            "type": "string"
        },
        "restaurantWorkingHour": {
            "type": "WorkingHour",
            "required": false
        },
        "tags": {
            "type": [
                "String"
            ]
        }
    },
    "validations": [],
    "mixins": {},
    "relations": {
        "businessUsers": {
            "type": "hasAndBelongsToMany",
            "model": "BusinessUser"
        },
        "menuStructures": {
            "type": "hasMany",
            "model": "MenuStructure",
            "foreignKey": "restaurantId"
        },
        "menuItems": {
            "type": "hasMany",
            "model": "MenuItem",
            "foreignKey": "restaurantId"
        },
        "orders": {
            "type": "hasMany",
            "model": "Order",
            "foreignKey": "restaurantId"
        }
    },
    "acls": [
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__create__businessUsers"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__create__menuStructures"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__get__menuStructures"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__create__menuItems"
        },
        {
            "principalType": "ROLE",
            "principalId": "$authenticated",
            "permission": "ALLOW",
            "property": "__get__menuItems"
        }
    ]
}